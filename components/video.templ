package components

templ Video(clipURL string) {
    <video
        id="my-video"
        class='video-js'
        data-setup='{}'
        controls
        fluid="true"
        preload="auto"
        >
        <source src={ clipURL } type="application/x-mpegURL"/>
    </video>
    <script src="/assets/index.js">
        let player = videojs("my-video");
        player.play()
    </script>
}